<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--dao层接口的全路径-->
<mapper namespace="com.aaa.rent.dao.UserDao">
    <select id="getUserList" resultType="com.aaa.rent.entity.User">
        select * from user
    </select>
    <select id="getUserByName" resultType="com.aaa.rent.entity.User">
        select * from user where username=#{username}
    </select>
</mapper>
